<% @friend_requests.each do |fr| %>
<p>You have a friend request from <%= fr.name %></p>
<p><%= link_to 'Accept Friend Request', accept_request_path(fr)%>
<p><%= link_to 'Decline Friend Request', decline_request_path(fr)%>
<% end %>

<% p "The number of friend requests is: " %>
<% p @friend_requests.length %>
